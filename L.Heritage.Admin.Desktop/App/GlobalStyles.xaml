<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="ColorWhite1">#FFFFFF</Color>
    <Color x:Key="ColorWhite2">#fcf3ca</Color>
    
    <Color x:Key="ColorRed2">#922E2E</Color>

    <Color x:Key="ColorYellow1">#F4C85D</Color> 

    <SolidColorBrush x:Key="BrushWhite1" Color="{StaticResource ColorWhite1}" />
    <SolidColorBrush x:Key="BrushWhite2" Color="{StaticResource ColorWhite2}" />

    <SolidColorBrush x:Key="BrushRed2" Color="{StaticResource ColorRed2}" />

    <SolidColorBrush x:Key="BrusYellow1" Color="{StaticResource ColorYellow1}" />

</ResourceDictionary>
    